{
    "compilerOptions": {
        "baseUrl": ".",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "paths": {
            "@Resources/*": [
                "./src/resources/*"
            ],
            "@Helper/*": [
                "./src/helper/*"
            ],
            "@Services/*": [
                "./src/services/*"
            ],
            "@Types/*": [
                "./src/types/*"
            ]
        }
    }
}